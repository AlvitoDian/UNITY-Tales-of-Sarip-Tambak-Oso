    sound: {fileID: 8300000, guid: 8c9e0b2604baf1b48a9b09746a8fddfd, type: 3}
  - text: ' Sarip, barang berharga ini dari mana?'
    speaker: {fileID: 11400000, guid: 31ce82b7e7488d549a9ef9f3d9569af9, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 31ce82b7e7488d549a9ef9f3d9569af9, type: 2}
